class User {
	private final String name;
	private static int totalUserAmount;
	private static String country;

	public User(String name) {
		this.name = name;
		totalUserAmount++;
		this.country = "Latvia";
	}

	public String getName() {
		return name;
	}

	public String toString() {
		return "User [name=" + this.name + "] totalUsers=" + totalUserAmount;
	}
}

public class DrivingRuleTask {
	private static final int SPEED_IN_TOWN = 50;
	
	

	/**
	 * @param args
	 */
	public static void main(String[] args) {
	
		
		new User("Vasilij");
		new User("Vasilij");
		new User("Vasilij");
		User vasilij = new User("Vasilij");
		System.out.println(vasilij.toString());
	}
}